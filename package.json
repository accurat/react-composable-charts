    ".": {
      "import": "./dist/react-composable-charts.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "np"
  },
  "peerDependencies": {
    "d3": "^7.3.0",
    "react": ">=16"
  },
  "homepage": "https://accurat/react-composable-charts",
  "repository": {
    "type": "git",
    "url": "https://github.com/accurat/react-composable-charts.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-composable-charts",
  "author": "Ivan Rossi",
  "size-limit": [
    {
      "path": "dist/react-composable-charts.cjs.production.min.js",
